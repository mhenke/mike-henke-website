{
  "name": "mike-henke-website",
  "version": "1.0.0",
  "description": "This is a personal portfolio website for Mike Henke, showcasing his skills, experience, and portfolio projects. The website is built using HTML and CSS, following specific design and coding guidelines.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf _site",
    "start": "NODE_ENV=development npx @11ty/eleventy --serve --watch",
    "dev": "NODE_ENV=development npx @11ty/eleventy --serve --watch",
    "build": "NODE_ENV=production npx @11ty/eleventy && npx pagefind --site _site --verbose",
    "build:dev": "NODE_ENV=development npx @11ty/eleventy",
    "build:purge": "NODE_ENV=production npx @11ty/eleventy",
    "serve": "npx @11ty/eleventy --serve",
    "dev:nosearch": "npm run clean && eleventy --serve --watch"
  },
  "keywords": [
    "portfolio",
    "blog",
    "eleventy",
    "cfwheels",
    "coldfusion"
  ],
  "author": "Mike Henke",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.0",
    "eleventy-plugin-purgecss": "^0.5.0",
    "glob": "^11.0.3",
    "luxon": "^3.6.1",
    "pagefind": "^1.1.0"
  }
}
